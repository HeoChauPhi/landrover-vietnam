{% block footer %}
  <footer id="footer" class="footer">
    <div class="container">
      <div class="footer-contact">
        {{ acfwidget('content_footer', 'footer_widget-2') }}
      </div>
      <div class="footer-logo">
        <img src="{{ acfwidget('logo_footer', 'footer_widget-2') }}" />
      </div>
      <div class="block-popup-footer">
        <div class="icon-popup fa fa-times">icon popup</div>
        <div class="popup-footer-content">
          {{ acfwidget('popup_footer', 'footer_widget-2') }}
        </div>
      </div>
    </div>
  </footer>
{% endblock %}
